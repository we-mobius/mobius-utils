{
  "name": "@we-mobius/mobius-utils",
  "version": "1.2.0",
  "description": "Probably the most comprehensive, powerfull tools library for JavaScript & TypeScript.",
  "type": "module",
  "typings": "./typings/main.d.ts",
  "types": "./typings/main.d.ts",
  "main": "./release/modules/cjs/main.cjs",
  "module": "./release/modules/esm/main.js",
  "exports": {
    ".": {
      "require": "./release/modules/cjs/main.cjs",
      "import": "./release/modules/esm/main.js",
      "node": "./release/modules/esm/main.js",
      "default": "./release/modules/esm/main.js"
    },
    "./es": "./release/modules/es/main.js",
    "./es/*": "./release/modules/es/*",
    "./es-js/*": "./release/modules/es/*.js",
    "./ts": "./src/main.ts",
    "./ts/*": "./src/ts/*.ts",
    "./ts-js/*": "./src/ts/*.js",
    "./esm": "./release/modules/esm/main.js",
    "./umd": {
      "require": "./release/modules/umd/main.cjs",
      "import": "./release/modules/umd/main.js",
      "node": "./release/modules/umd/main.js",
      "default": "./release/modules/umd/main.js"
    },
    "./cjs": "./release/modules/cjs/main.cjs",
    "./src/*": "./src/*",
    "./src-ts/*": "./src/*.ts",
    "./src-js/*": "./src/*.js",
    "./statics/*": "./statics/*",
    "./release/*": "./release/*",
    "./release-js/*": "./release/*.js",
    "./release-cjs/*": "./release/*.cjs",
    "./esm/*": "./release/modules/esm/*",
    "./cjs/*": "./release/modules/cjs/*",
    "./umd/*": "./release/modules/umd/*",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "es": [
        "./release/modules/es/main.d.ts"
      ],
      "es/*": [
        "./release/modules/es/*"
      ],
      "es-js/*": [
        "./release/modules/es/*"
      ],
      "ts": [
        "./src/main.ts"
      ],
      "ts/*": [
        "./src/ts/*"
      ],
      "ts-js/*": [
        "./src/ts/*"
      ],
      "esm": [
        "./typings/main.d.ts"
      ],
      "umd": [
        "./typings/main.d.ts"
      ],
      "cjs": [
        "./typings/main.d.ts"
      ],
      "*": [
        "./typings/main.d.ts"
      ]
    }
  },
  "keywords": [
    "mobius",
    "utils",
    "functional",
    "stream",
    "atom"
  ],
  "directories": {
    "doc": "./docs",
    "bin": "./bin"
  },
  "scripts": {
    "cz-commit": "npm run lint && npx git-cz",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "dev": "node ./scripts/dev",
    "build": "node ./scripts/build",
    "dist": "node ./scripts/dist",
    "serve:build": "npx live-server ./build --port=0624",
    "serve:dist": "npx live-server ./dist --port=0624",
    "expose": "npx ngrok http 624",
    "bundle": "node ./scripts/bundle",
    "doc": "node ./scripts/document",
    "beforepublish": "npm run build && npm run dist && npm run bundle && npm run doc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/we-mobius/mobius-utils.git"
  },
  "author": {
    "name": "kongxiangyan",
    "email": "kcigaret@outlook.com",
    "url": "https://www.cigaret.world/"
  },
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/we-mobius/mobius-utils/issues"
  },
  "homepage": "https://github.com/we-mobius/mobius-utils#readme",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/eslint-parser": "^7.11.5",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@commitlint/cli": "^8.3.5",
    "@types/javascript-state-machine": "^2.4.3",
    "@types/rx": "^4.1.2",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.2.1",
    "commitlint-config-cz": "^0.13.2",
    "copy-webpack-plugin": "^5.1.2",
    "cz-customizable": "^6.3.0",
    "eslint": "^7.9.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "fs-extra": "^9.0.1",
    "handlebars": "^4.7.6",
    "handlebars-loader": "^1.7.1",
    "html-webpack-plugin": "^4.4.1",
    "husky": "^4.3.0",
    "lint-staged": "^10.3.0",
    "mini-svg-data-uri": "^1.3.3",
    "terser-webpack-plugin": "^5.1.2",
    "ts-loader": "^9.2.5",
    "typedoc": "^0.22.10",
    "typescript": "4.5",
    "webpack": "^5.10.3",
    "webpack-bundle-analyzer": "^4.2.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.10.3",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^5.7.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    }
  },
  "jsdelivr": "./release/modules/umd/main.js",
  "files": [
    "/release/*",
    "/dist/*",
    "/build/*",
    "/src/*",
    "/public/*",
    "/docs/*",
    "/bin/*",
    "/typings/*"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "sideEffects": false,
  "dependencies": {
    "axios": "^0.24.0",
    "javascript-state-machine": "^3.1.0",
    "nanoid": "^3.1.30",
    "uuid": "^8.3.2"
  }
}
