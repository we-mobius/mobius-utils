{
  "name": "@we-mobius/mobius-utils",
  "version": "0.3.1",
  "description": "Mobius utils.",
  "type": "module",
  "main": "./src/main.js",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./src/main.js",
      "default": "./src/main.js"
    },
    "./module": "./src/main.js",
    "./es": "./src/main.js",
    "./cjs": "./dist/cjs/index.js",
    "./module/": "./src/es/",
    "./es/": "./src/es/",
    "./cjs/": "./src/cjs/",
    "./src/": "./src/",
    "./dist/": "./dist/"
  },
  "module": "./src/main.js",
  "keywords": [
    "mobius",
    "utils",
    "functional"
  ],
  "directories": {
    "doc": "./docs",
    "bin": "./bin"
  },
  "scripts": {
    "cz-commit": "npm run lint && npx git-cz",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "dev": "node ./scripts/dev",
    "build": "node ./scripts/build",
    "dist": "node ./scripts/dist",
    "serve:build": "npx live-server ./build --port=0624",
    "serve:dist": "npx live-server ./dist --port=0624",
    "expose": "npx ngrok http 624",
    "bundle": "node ./scripts/bundle",
    "beforepublish": "npm run build && npm run dist && npm run bundle"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/we-mobius/mobius-utils.git"
  },
  "author": {
    "name": "kongxiangyan",
    "email": "kcigaret@outlook.com",
    "url": "http://cigaret.world/"
  },
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/we-mobius/mobius-utils/issues"
  },
  "homepage": "https://github.com/we-mobius/mobius-utils#readme",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/eslint-parser": "^7.11.5",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@commitlint/cli": "^8.3.5",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.2.1",
    "commitlint-config-cz": "^0.13.2",
    "copy-webpack-plugin": "^5.1.2",
    "cz-customizable": "^6.3.0",
    "eslint": "^7.9.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "fs-extra": "^9.0.1",
    "handlebars": "^4.7.6",
    "handlebars-loader": "^1.7.1",
    "html-webpack-plugin": "^4.4.1",
    "husky": "^4.3.0",
    "lint-staged": "^10.3.0",
    "mini-svg-data-uri": "^1.3.3",
    "terser-webpack-plugin": "^5.1.2",
    "webpack": "^5.10.3",
    "webpack-bundle-analyzer": "^4.2.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.10.3",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^5.7.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    }
  },
  "jsdelivr": "./release/mobius-utils.js",
  "files": [
    "/release/*",
    "/dist/*",
    "/build/*",
    "/src/*",
    "/public/*",
    "/docs/*",
    "/bin/*"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "sideEffects": false,
  "dependencies": {
    "axios": "^0.21.0"
  }
}
